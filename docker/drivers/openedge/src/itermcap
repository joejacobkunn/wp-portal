	:kr=\E[C:	:L_kr=<Right>:\
	:bc=\177:\
	:kl=\E[D:	:L_kl=<Left>:\
	:kh=\E[23~h:	:L_kh=<F11> h:\
	:EN=\E[23~e:	:L_EN=<F11> e:\
	:PU=\E[5~:	:L_PU=<Prev Screen>:\
	:PD=\E[6~:	:L_PD=<Next Screen>:\
